{
  "name": "zembrowski/phpsms-poland",
  "description": "Send SMS via Polish mobile carriers using PHP.",
  "keywords": [ "sms", "poland", "orange"],
  "homepage": "https://github.com/zembrowski/phpsms-poland",
  "license": "MIT",
  "authors": [
    {
      "name": "Krzysztof Tomasz Zembrowski",
      "homepage": "http://krzysztof.zembrowski.com/"
    }
  ],
  "require": {
    "php": ">=5.6",
    "rmccue/requests": "~1.7",
    "emanueleminotto/simple-html-dom": "~1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "satooshi/php-coveralls": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "zembrowski\\SMS\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "zembrowski\\SMS\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
